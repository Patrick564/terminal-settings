alias gs='git status'
alias ga='git add'
alias gc='git commit'
alias gps='git push'
alias gpl='git pull'

alias nvm-d='nvm use default'
alias nvm-s='nvm use stable'

alias status-pg='sudo service postgresql status'
alias start-pg='sudo service postgresql start'
alias stop-pg='sudo service postgresql stop'
alias run-pg='sudo -u postgres psql'

alias status-mg='sudo service mongodb status'
alias start-mg='sudo service mongodb start'
alias stop-mg='sudo service mongodb stop'
